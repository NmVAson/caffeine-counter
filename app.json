{
  "expo": {
    "name": "Caffeine Counter",
    "slug": "caffeine-counter",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nmvason.caffeinecounter"
    },
    "android": {
      "package": "com.nmvason.caffeinecounter"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}